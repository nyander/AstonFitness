{
  "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "mocha": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-console": "off",
      "eqeqeq": "error",
      "no-magic-numbers": "error",
      "no-use-before-define": "error",
      "semi": "error",
      "no-unused-vars": "warn"
    }
}
